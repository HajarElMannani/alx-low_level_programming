#include "main.h"
/**
 *_strcmp - compares two strings
 *@s1: first string to compare
 *@s2: second string to compare
 *Return: 0 when equal,positive when s1 grater than s2,
 * negative when s1 is less than s2
 */
int _strcmp(char *s1, char *s2)
{
int i, j;
j = 0;
j = 0;
while (s1[i] != '\0')
{
i++;
}
while (s2[j] != '\0')
{
j++;
}
if (i == j)
return (0);
else if (i < j || i > j)
return (i - j);
}
